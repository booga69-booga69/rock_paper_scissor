def numero_pick():
  rps_dict = {0: 'paper', 1: 'rock', 2: 'scissors'}
  return rps_dict()

def pick_numero():
  rps_dict = {'paper': 0, 'rock': 1, 'scissors': 2}
  return rps_dict()

return random.choice(['rock', 'paper', 'scissors'])
